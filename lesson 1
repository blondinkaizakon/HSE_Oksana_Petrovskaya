string getFormatedTime(int seconds)
{
    int s, m, h;
    string formatedTime;
    s = seconds%60;
    h = seconds/3600;
    m = seconds/60 - h*60;
 
    return h.ToString() + ":" + m.ToString() + ":" +  s.ToString();
